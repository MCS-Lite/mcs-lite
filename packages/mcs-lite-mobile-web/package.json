{
  "name": "mcs-lite-mobile-web",
  "version": "0.4.12",
  "description": "MCS Lite Mobile Web",
  "repository": "MCS-Lite/mcs-lite",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "bugs": {
    "url": "https://github.com/MCS-Lite/mcs-lite/issues",
    "email": "evenchange4@gmail.com"
  },
  "license": "MIT",
  "homepage": "http://localhost:3000/mobile",
  "files": [
    "build"
  ],
  "proxy": {
    "/api": {
      "target": "http://localhost:3000"
    },
    "/oauth": {
      "target": "http://localhost:3000"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom --coverage --runInBand",
    "test:watch": "react-scripts test --env=jsdom",
    "extract:messages": "react-intl-cra 'src/**/*.js' '../mcs-lite-translation/src/mcs-lite-mobile-web/messages.json'",
    "extract:pot": "react-intl-po json2pot '../mcs-lite-translation/src/mcs-lite-mobile-web/messages.json' -o '../mcs-lite-translation/src/mcs-lite-mobile-web/messages.pot'",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@cycle/http": "^14.8.0",
    "@cycle/rxjs-run": "^7.3.0",
    "@cycle/time": "^0.8.0",
    "browser-locale": "^1.0.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "history": "^3.2.1",
    "jest-styled-components": "^4.9.0",
    "mcs-lite-connect": "^0.3.9",
    "mcs-lite-icon": "^0.3.8",
    "mcs-lite-theme": "^0.3.7",
    "mcs-lite-translation": "^0.1.7",
    "mcs-lite-ui": "^0.4.10",
    "polished": "^1.8.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-intl-cra": "^0.2.8",
    "react-intl-inject-hoc": "^0.2.9",
    "react-intl-po": "^2.1.1",
    "react-loadable": "^5.3.1",
    "react-motion-ui-pack": "^0.10.3",
    "react-overlays": "^0.8.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.3",
    "react-scripts": "^1.0.14",
    "react-text-truncate": "^0.12.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-cycles": "^0.4.1",
    "redux-freeze": "^0.1.5",
    "redux-mock-store": "^1.3.0",
    "rxjs": "^5.5.2",
    "styled-components": "^2.2.3",
    "uuid": "^3.1.0"
  }
}

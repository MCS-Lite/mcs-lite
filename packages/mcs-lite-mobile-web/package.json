{
  "name": "mcs-lite-mobile-web",
  "version": "0.1.1",
  "description": "MCS Lite Mobile Web",
  "repository": "evenchange4/mcs-lite",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "bugs": {
    "url": "https://github.com/evenchange4/mcs-lite/issues",
    "email": "evenchange4@gmail.com"
  },
  "homepage": "http://localhost:3000/mobile",
  "files": [
    "build"
  ],
  "proxy": "http://localhost:3000",
  "scripts": {
    "clean": "rm -rf build",
    "start": "node -r dotenv/config ./node_modules/.bin/react-scripts start dotenv_config_path=.env.development",
    "prebuild": "npm run clean",
    "build": "node -r dotenv/config ./node_modules/.bin/react-scripts build dotenv_config_path=.env.production",
    "test": "CI=true react-scripts test --env=jsdom --coverage --runInBand",
    "test:watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extract:messages": "react-intl-cra './src/**/*.js' './messages.json'",
    "extract:pot": "react-intl-po json2pot './messages.json' -o './mcs-lite-mobile-web.pot'",
    "prepublish": "rm -rf ./build/static/*/*.map"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "date-fp": "^4.4.8",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "history": "^3.2.1",
    "mcs-lite-fetch-rx": "^0.1.0",
    "mcs-lite-icon": "^0.1.0",
    "mcs-lite-theme": "^0.0.1",
    "mcs-lite-ui": "^0.1.1",
    "normalize.css": "^5.0.0",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-intl": "^2.2.3",
    "react-intl-cra": "^0.1.0",
    "react-intl-inject-hoc": "^0.1.0",
    "react-intl-po": "^2.0.1",
    "react-motion-ui-pack": "^0.10.2",
    "react-overlays": "^0.6.11",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "react-scripts": "0.9.3",
    "react-text-truncate": "^0.8.3",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-freeze": "^0.1.5",
    "redux-mock-store": "^1.2.2",
    "redux-observable": "^0.13.0",
    "rxjs": "^5.2.0",
    "styled-components": "^1.4.3",
    "uuid": "^3.0.1"
  }
}

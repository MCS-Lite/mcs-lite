{
  "name": "mcs-lite-admin-web",
  "version": "0.2.13",
  "description": "MCS Lite Admin Web",
  "repository": "MCS-Lite/mcs-lite",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "bugs": {
    "url": "https://github.com/MCS-Lite/mcs-lite/issues",
    "email": "evenchange4@gmail.com"
  },
  "license": "MIT",
  "homepage": "http://localhost:3002/admin",
  "files": ["build"],
  "proxy": {
    "/api": {
      "target": "http://localhost:3002"
    },
    "/oauth": {
      "target": "http://localhost:3002"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build":
      "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom --coverage --runInBand",
    "test:watch": "react-scripts test --env=jsdom",
    "extract:messages":
      "react-intl-cra 'src/**/*.js' -o '../mcs-lite-translation/src/mcs-lite-admin-web/messages.json'",
    "extract:pot":
      "react-intl-po json2pot '../mcs-lite-translation/src/mcs-lite-admin-web/messages.json' -o '../mcs-lite-translation/src/mcs-lite-admin-web/messages.pot'",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@cycle/http": "14.9.0",
    "@cycle/rxjs-run": "8.0.0",
    "@cycle/storage": "5.0.3",
    "@cycle/time": "0.8.0",
    "browser-locale": "1.0.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.1",
    "flow-bin": "0.67.1",
    "history": "3.3.0",
    "jest-styled-components": "next",
    "mcs-lite-icon": "^0.3.22",
    "mcs-lite-theme": "^0.3.20",
    "mcs-lite-translation": "^0.1.20",
    "mcs-lite-ui": "^0.7.0",
    "polished": "1.9.2",
    "promise-file-reader": "1.0.2",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-codemirror":
      "git://github.com/skidding/react-codemirror.git#106-fix-update",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-intl-cra": "^0.3.3",
    "react-intl-inject-hoc": "^0.2.23",
    "react-intl-po": "2.2.0",
    "react-loadable": "5.3.1",
    "react-motion-ui-pack": "0.10.3",
    "react-overlays": "0.8.3",
    "react-redux": "5.0.7",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "0.4.4",
    "react-scripts": "1.1.1",
    "react-topbar-progress-indicator": "2.0.0",
    "react-virtualized": "9.18.5",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-cycles": "0.4.1",
    "redux-freeze": "0.1.5",
    "redux-mock-store": "1.5.1",
    "rxjs": "5.5.7",
    "styled-components": "3.2.1",
    "uuid": "3.2.1",
    "validator": "9.4.1"
  }
}
